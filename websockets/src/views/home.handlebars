<h1>Lista de Productos </h1>
<div id="productos">
    {{#each products}}

    <div class="product">
        <ul>
            <li>Producto: {{this.title}}</li>
            <li>Descripcion:{{this.description}}</li>
            <li>Precio: {{this.price}}</li>
        </ul>
    </div>
    {{/each}}
</div>